// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(cuid())
  user  String @unique
  pass  String
  name  String
  email String
  // Result Result[]
  // Log    Log[]

  @@unique([user, pass])
}

